<h1>Edit Product</h1>

<%= form_for @product do |form| %>
<br>
<%= form.label :name %>
<%= form.text_field :name %>
<br>
<%= form.label :description %>
<%= form.text_field :description %>
<br>
<%= form.label :photo %>
<%= form.text_field :photo %>
<br>
<%= form.label :price %>
<%= form.text_field :price %>
<br>
<%= form.submit "Save Changes" %>
	<% end %>